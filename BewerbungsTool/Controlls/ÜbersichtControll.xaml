<UserControl x:Class="BewerbungsTool.Controlls.ÜbersichtControll"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BewerbungsTool.Controlls"
             xmlns:ck="clr-namespace:CKCustomControlls;assembly=CKCustomControlls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">




    <Grid d:Background="White">

        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Button  Height="40" Width="100" Style="{StaticResource CkButton}" Content="TestCV" Command="{Binding TestCVCommand}" Background="{StaticResource GUIForgroundMidBrush}"/>
        <Button Grid.Row="1" Height="40" Width="100" Style="{StaticResource CkButton}" Content="TestAnschreiben" Command="{Binding TestAnschreibenCommand}" Background="{StaticResource GUIForgroundMidBrush}"/>
        

    </Grid>

</UserControl>
