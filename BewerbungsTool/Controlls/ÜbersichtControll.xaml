<UserControl x:Class="BewerbungsTool.Controlls.ÜbersichtControll"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BewerbungsTool.Controlls"
             xmlns:ck="clr-namespace:CKCustomControlls;assembly=CKCustomControlls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">




    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Button Content="TestLatex" Command="{Binding TestLatexCommand}" />

        <local:LebenslaufStatList Grid.Row="1" DataContext="{Binding LebenslaufStatsListViewModel}"/>

    </Grid>

</UserControl>
